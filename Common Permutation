#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main(){
	string a,b;
	
	while(getline(cin,a),getline(cin,b)){
	sort(a.begin(),a.end());//getline不用vector
	sort(b.begin(),b.end());//getline不用vector
	//cout<<a<<endl<<b<<endl;
	int i=0;	
	int j=0;	
		while(i<a.length()&&j<b.length()){
			if(a[i]==b[j]){
				cout<<a[i];
				i++;
				j++;
			}
		
			else{
				if(a[i]<b[j])
				i++;
				else
				j++;
			}
			
		}
		
	cout<<endl;
	
	}
return 0;
}
